{
  "name": "TKRtape",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "lint-staged",
    "reactjs": "npm start --prefix reactjs",
    "start": "npm start --prefix nodejs",
    "install-reactjs": "npm install --prefix reactjs",
    "install-nodejs": "npm install --prefix nodejs",
    "build-reactjs": "npm run build --prefix reactjs",
    "build-nodejs": "npm run db-update --prefix nodejs",
    "nodejs": "npm run dev --prefix nodejs",
    "dev": "concurrently --kill-others-on-fail \"npm run nodejs\" \"npm run reactjs\"",
    "heroku-postbuild": "concurrently \"npm:install-*\"; concurrently \"npm:build-*\"; "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fs-tech-degs/capstone-project-Austin-Wise.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier — single-quote — write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.4",
    "prettier": "^1.18.2"
  }
}
